# URL of the FastAPI backend (no trailing slash)
NEXT_PUBLIC_API_URL=http://localhost:8000

# Event date shown on the hero section (e.g. "Saturday, 15 March 2026")
# Set to TBD to hide the date badge
NEXT_PUBLIC_EVENT_DATE=TBD

# Price per Lamprais (must match PRICE_PER_ITEM in backend .env)
NEXT_PUBLIC_PRICE_PER_ITEM=20

# Currency label shown in the UI
NEXT_PUBLIC_CURRENCY=AUD
